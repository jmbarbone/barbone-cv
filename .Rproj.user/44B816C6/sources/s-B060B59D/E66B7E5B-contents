r_profile <- "~/.Rprofile"

if (interactive() & file.exists(r_profile)) {
  source(r_profile, print.eval = TRUE)
}

# Index has some initial set up that I'd like to have available
jordan::ksource("index.Rmd")
# Cleans up package usages so I don't have conflicts (e.g., stats::filter())
jordan::.SendAttachedPackagesToREnviron()
